{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LdapOptions": {
    "Server": {
      "Host": "your-ldap-server",
      "Port": 389
    },
    "BindDn": "your-bind-dn",
    "BindCredentials": "your-bind-password",
    "SearchBase": "your-search-base",
    "SearchFilter": "your-search-filter"
  }
}
